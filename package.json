{
  "name": "gatsby-starter-contentful-homepage",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "biome lint .",
    "lint:fix": "biome format --write .",
    "gatsby-provision": "gatsby-provision-contentful --contentful-data-path='./scripts/data.json' --space-id=$CONTENTFUL_SPACE_ID --management-token=$CONTENTFUL_MANAGEMENT_TOKEN",
    "setup": "node ./scripts/setup.js",
    "export-content": "contentful space export --space-id=$SPACE_ID --management-token=$MANAGEMENT_TOKEN --export-dir=scripts --content-file=data.json"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.15.1",
    "@fontsource/dm-mono": "^4.5.10",
    "@fontsource/dm-sans": "^4.5.9",
    "@iconscout/react-unicons": "^1.1.6",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/css-utils": "^0.1.6",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "gatsby": "^5.14.6",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-vanilla-extract": "^4.0.2",
    "gatsby-remark-gifs": "^1.2.0",
    "gatsby-source-contentful": "^8.15.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "is-absolute-url": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@tailwindcss/postcss": "^4.1.11",
    "chalk": "^4.1.2",
    "chart.js": "^4.5.0",
    "contentful-cli": "^1.19.1",
    "contentful-import": "^8.5.63",
    "gatsby-provision-contentful": "^0.0.5",
    "husky": "^7.0.4",
    "inquirer": "^8.2.6",
    "lint-staged": "^12.5.0",
    "yargs-parser": "^21.1.1"
  }
}